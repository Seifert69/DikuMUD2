race RACE_MARDAGG
sex SEX_MALE
height 310
weight 300
NATURAL_DEF(WPN_FIST, ARM_PLATE)
MSET_ABILITY(20,17,7,20,10,6,10,10)
MSET_WEAPON(5,5,4,15,7,4)
MSET_SPELL(1,1,1,15,6,1,1,11,11,11,1)
special SFUN_RANDOM_GLOBAL_MOVE time PULSE_SEC*40 bits SFB_RANTIME

dilbegin ra();
var
	pc : unitptr;
	random : integer;
	timer : integer;
	timerdly : integer;
	unitcnt : integer;
	cnt : integer;

code
{
	timer := 0;
	timerdly := (rnd(2,10));
	on_activation(self.position == POSITION_FIGHTING,fight);
:start:
	heartbeat:=PULSE_SEC*1;
	pause;
	if (self.position <= POSITION_STUNNED) goto start;
	if (self.position != POSITION_FIGHTING) self.position := POSITION_STANDING;
	if (timer == timerdly)
	{
		act("$1n utters the words 'zaxh xnijha yadafah'.",A_ALWAYS,self,null,null,TO_ALL);
		act("Suddenly $1n disappears in a cloud of dark dust.",A_ALWAYS,self,null,null,TO_ALL);
		if (self.zone != "ishlor")
		{
			link(self, findroom("split_2@ishlor"));
			act("$1n appears in a puff of black smoke.",A_ALWAYS,self,null,null,TO_ALL);
			timerdly := (rnd(20,50));
			timer := -1;
			goto start;
		}
		random := rnd(1,3);
		if (random == 1) link(self, findroom("market_sq@midgaard"));
		if (random == 2) link(self, findroom("ent_gate@garden"));
		if (random == 3) link(self, findroom("vil_path1@urland"));
		act("$1n appears in a puff of black smoke.",A_ALWAYS,self,null,null,TO_ALL);
		timerdly := (rnd(1,3));
		timer := -1;
	}
	heartbeat := PULSE_SEC * (rnd(15,30));
	wait(SFB_TICK, TRUE);
	unitcnt := 0;
	timer := timer + 1;
	cnt := 0;
:newname:
	heartbeat := PULSE_SEC * 1;
	pause;
	if( self.zone in {"ishlor","demon_realm"} )
	{
		pc := findrndunit(self,FIND_UNIT_SURRO,UNIT_ST_PC);
		unitcnt := unitcnt + 1;
		if (unitcnt >= 10) goto start;
		if (pc.level > IMMORTAL_LEVEL) goto newname;
		if (isaff(pc,ID_SANCTUARY)) goto newname;
		secure(pc,newname);
		if (cnt == 0) act("$1n screams 'PATHETIC MORTAL! YOU'RE MINE!!!'",A_ALWAYS,self,null,null,TO_ALL);
		cnt := 1;
		exec("kill "+pc.name,self);
		goto fight2;
	}
	goto start;

	:fight:
	heartbeat := PULSE_SEC * 3;
	pc := self.fighting;
	secure(pc,start);
	pause;
	exec("moo", self);
	pause;
	exec("say MORTAL FOOLS! You can't kill me! I am almost an immortal!", self);
	pause;
	exec("grin " + pc.name, self);
	pause;
	pause;
	exec("say Prepare to DIE! Dissbelieving mortals!", self);
	pause;
	exec("moo", self);
	pause;
	pause;
	exec("say Doomsday is here!! Your time is up mortal! NOW YOU DIE!", self);
	pause;
	exec("moo",self);
	pause;
	pause;
	exec("slap "+pc.name,self);
:fight2:
	pause;
	if (pc.position <= POSITION_SITTING) exec("kick "+pc.name,self);
	if (self.position == POSITION_FIGHTING) goto fight2;
	unsecure(pc);
	goto fight;

}
dilend
end
