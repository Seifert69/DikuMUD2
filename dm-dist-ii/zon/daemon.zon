	inhaas
names {"inhaas"}
title "Inhaas"
descr "Inhaas the creator is standing here."
extra {}
"He look's like a normal human. He seems to be arond 65 years old with long
white hair and a big white cloak wrapped around his body."

	level 65
	exp 100
	alignment -400
	race RACE_HUMAN
	sex SEX_MALE
	height 180
	weight 160
	money 3 SILVER_PIECE
	NATURAL_DEF(WPN_FIST, ARM_CHAIN)
	MSET_ABILITY(12,10,11,20,10,10,12,15)
	MSET_WEAPON(0,0,0,0,0,0)
	MSET_SPELL(20,10,10,5,5,10,20,5,5,5,5)

dilbegin inhaas();
var
	pc : unitptr;
	wraith : unitptr;
code
{
	on_activation(self.position == POSITION_FIGHTING,loop);

:start:
	heartbeat := PULSE_SEC * 6;
	pause;
	pc := findrndunit(self,FIND_UNIT_SURRO,UNIT_ST_PC);
	if ( pc == null ) goto start;
	if (pc.level >= IMMORTAL_LEVEL) goto start;
	secure(pc,start);
	wraith := findsymbolic("wraith@ishlor");
	if (wraith == null) goto nowraith;
	if (wraith.position <= POSITION_SLEEPING) goto nowraith;
	if (wraith.outside.idx != self.outside.idx) goto nowraith;
	exec("say Hey! What are you doing here "+pc.name+"?",self);
	pause;
	exec("say This is a private area! Go away!",self);
	pause;
	exec("say Go away now! Or i will have the wraith kill you!",self);
	pause;
	exec("grin "+pc.name,self);
	pause;
	exec("say As you wish! Prepare to die moron!!!",self);
	heartbeat := PULSE_SEC * 3;
	pause;
	exec("emote whispers something to the wraith!",self);
	heartbeat := PULSE_SEC * 2;
	pause;
	sendto(pc.name, findsymbolic("wraith@ishlor"));
	goto loop;
:nowraith:
	exec("say No!! Please don't kill me "+pc.name+"!",self);
	pause;
	exec("say I'm just an incompetent wussy... please!",self);
	pause;
	exec("cry "+pc.name,self);
	pause;
	exec("say I beg you to spare my life..",self);
	pause;
	exec("wor "+pc.name,self);
	pause;
	exec("smile",self);
	pause;
:loop:
	heartbeat := PULSE_SEC * 2;
	pause;
	if (self.position == POSITION_FIGHTING) goto loop;
	if (wraith == null) goto loop2;
	if (wraith.position == POSITION_FIGHTING) goto loop;
:loop2:
	heartbeat := PULSE_SEC * 60;
	pause;
	goto start;

}
dilend
end
