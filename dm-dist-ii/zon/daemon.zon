	d_scythe
names {"scythe of death","scythe"}
title "the scythe of death"
descr "A sharp looking scythe with a glowing aura is lying here."
flags {UNIT_FL_MAGIC}
extra {}
"The Scythe looks extremly sharp. A green aura is glowing arond it."
manipulate {MANIPULATE_TAKE, MANIPULATE_WIELD}

WEAPON_DEF(WPN_SCYTHE, +1, 1)
MATERIAL_METAL("A dull black metal")
STR_TRANSFER(+3)
DEX_TRANSFER(+2)
CHA_TRANSFER(-3)
weight 30
cost 1444 "cp"
rent 722  "cp"
dilcopy ali_restrict@function (-1000,-350,0,25,"");
dilcopy level_restrict@function (50,0,25,"");
spell 70

dilbegin  deathscythe();
var
	n : integer;
code{
   :loop:
     wait( SFB_CMD, command( "slay" ) );
     if( activator != self.outside ) goto loop;
     if( self.equip != WEAR_WIELD ) goto loop;
     block;
     if( self.alignment <= 0 )
     {
      act( "Your $2n glows slightly, but nothing happens.", A_SOMEONE, self.outside, self, null, TO_CHAR );
      goto loop;
     }
     self.alignment := self.alignment - 1;
     n := cast_spell( SPL_FIREBALL_1, self.outside, self, self.outside.fighting, "scythe_dam@ishlor" );
     goto loop;
}
dilend

dilcopy recharge();

dilbegin slay_dil();
code{
   :loop:
     wait( SFB_COM, command( CMD_AUTO_COMBAT ) );
     if( self.equip != WEAR_WIELD ) goto loop;
     if( self.outside.fighting == null ) goto loop;
     if( self.outside.fighting.hp > 10 ) goto loop;
     act( "$1n's $2N rips $3n into pieces.", A_SOMEONE, self.outside, self, self.outside.fighting, TO_REST );
     act( "Your $2n rips $3n into pieces.", A_SOMEONE, self.outside, self, self.outside.fighting, TO_CHAR );
     act( "$1n's $2N rips you into pieces, it's time to see odin.", A_SOMEONE, self.outside, self, self.outside.fighting, TO_VICT );
     self.outside.fighting.hp := -11;
     goto loop;
}
dilend

end
