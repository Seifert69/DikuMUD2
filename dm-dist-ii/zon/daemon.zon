%dil

dilbegin recharge();
code{
     heartbeat := PULSE_SEC * 60 * 5;
   :loop:
     pause;
     if( self.equip == 0 ) goto loop;
     if( self.alignment >= 12 ) goto loop;
     act( "$1n's $2n glows slightly.", A_SOMEONE, self.outside, self, null, TO_REST );
     act( "Your $2n glows slightly.", A_SOMEONE, self.outside, self, null, TO_CHAR );
     self.alignment := self.alignment + 1;
     goto loop;
}
dilend

dilbegin why_grass();
code{
   :start:
    wait( SFB_CMD, command( CMD_GET ) and ( argument == "grass" ) );
    act("No way! Why do you want some grass??",A_ALWAYS,activator,null,null,TO_CHAR);
    block;
    goto start;
}
dilend


dilbegin scythe_dam( medi : unitptr, tar : unitptr, hm : integer );
var
	random : integer;
	enemy : unitptr;
code
{
	enemy := self.outside.fighting;
	if( hm < 20 )
	{
		act("Your $2N SMASHES $3n HARD!", A_ALWAYS,self,medi,tar,TO_CHAR);
		act("$1n's $2N SMASHES you HARD!",A_ALWAYS,self,medi,tar,TO_VICT);
		act("$1n's $2N SMASHES $3n HARD!",A_ALWAYS,self,medi,tar,TO_NOTVICT);
		return;
	}
	if( hm < 30 )
	{
		act("$3n screams out in pain as you MUTILATE $3s legs with your $2N!", A_ALWAYS,self,medi,tar,TO_CHAR);
		act("A scream escapes your lips as $1n MUTILATES your legs with $1s $2N!",A_ALWAYS,self,medi,tar,TO_VICT);
		act("$3n screams out in pain as $1n MUTILATES $3s legs with $1s $2N!",A_ALWAYS,self,medi,tar,TO_NOTVICT);
		return;
	}
	if( hm < 40 )
	{
		act("You THRUST the blade on your $2N HARD into $3n's stomach!", A_ALWAYS,self,medi,tar,TO_CHAR);
		act("$1n THRUSTS the blade on $1s $2N HARD into your stomach!",A_ALWAYS,self,medi,tar,TO_VICT);
		act("$1n THRUSTS the blade on $1s $2N HARD into $3n's stomach!",A_ALWAYS,self,medi,tar,TO_NOTVICT);
		return;
	}

	if( hm >= 40 )
	{
		act("You brutally SLAUGHTER $3n with your $2N!", A_ALWAYS,self,medi,tar,TO_CHAR);
		act("$1n brutally SLAUGHTER you with $1s $2N!",A_ALWAYS,self,medi,tar,TO_VICT);
		act("That really did HURT!",A_ALWAYS,self,medi,tar,TO_VICT);
		act("$1n brutally SLAUGHTER $3n with $1s $2N!",A_ALWAYS,self,medi,tar,TO_NOTVICT);
		act("$3n screams out in pain!",A_ALWAYS,self,medi,tar,TO_NOTVICT);
		return;
	}
	return;
}
dilend

dilbegin dagger_dam( medi : unitptr, tar : unitptr, hm : integer );
code{
	return;
}
dilend
